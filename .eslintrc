{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "installedESLint": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "babel",
    "react"
  ],
  "rules": {
    "babel/object-curly-spacing": [
      "error",
      "always"
    ],
    "babel/object-shorthand": 1,
    "babel/flow-object-type": 1,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "no-console": 1,
    "quotes": [
      "error",
      "single"
    ],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "semi": [
      "error",
      "never"
    ],
    "strict": 0
  }
}
